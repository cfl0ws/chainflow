[Unit]
Description=Stargazer Exporter
Wants=network-online.target
After=network-online.target

[Service]
User=prometheus
Group=prometheus
Type=simple
ExecStart=/usr/local/bin/stargazer_exporter \
  --block-address=<change to your hash address> 
Restart=on-failure
RestartSec=20s

[Install]
WantedBy=multi-user.target
